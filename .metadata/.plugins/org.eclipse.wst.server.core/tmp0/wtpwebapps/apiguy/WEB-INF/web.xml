<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<!-- Servlet Context and Config allow us to store initialization parameters -->
	<!-- associated with our Servlet Container (context) and individual servlets 
		(config) -->
	<context-param>
		<param-name>cheese</param-name>
		<param-value>cheddar</param-value>
	</context-param>

	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.revature.LoginServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet>
		<servlet-name>ConfigExample</servlet-name>
		<servlet-class>com.revature.ConfigExampleServlet</servlet-class>
		<!-- This param is only associated with the ConfigExample Servlet: -->
		<init-param>
			<param-name>apple</param-name>
			<param-value>fuji</param-value>
		</init-param>
	</servlet>
	
	<servlet>
		<servlet-name>Register</servlet-name>
		<servlet-class>com.revature.RegisterServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>RegisterServlet</servlet-name>
		<display-name>RegisterServlet</display-name>
		<description></description>
		<servlet-class>com.revature.RegisterServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ConfigExample</servlet-name>
		<url-pattern>/config</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/registration</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/RegisterServlet</url-pattern>
	</servlet-mapping>

</web-app>
